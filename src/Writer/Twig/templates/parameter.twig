{%- autoescape false -%}
    {{- parameter.getType() | alias -}}
    {{- ' $' ~ parameter.getName() -}}
    {%- if parameter.getValue() -%}
        {{ ' = ' }}
        {{- include('expression.twig', { 'expression': parameter.getValue() }, with_context = false) -}}
    {%- endif -%}
{%- endautoescape -%}
