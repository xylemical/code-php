{%- autoescape false -%}
    {%- if not documentation.isEmpty() %}
        {{- "\n/**\n" -}}
        {%- for line in documentation.getContents() | split("\n") -%}
            {%- if line -%}
                {{ " * " ~ line ~ "\n" }}
            {%- else -%}
                {{ " *\n" }}
            {%- endif -%}
        {%- endfor -%}
        {{- " */\n" -}}
    {% else %}
        {{- "\n" -}}
    {% endif -%}
{%- endautoescape -%}
